            "email": "ahhardin@email.unc.edu"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "windwalker/renderer": "~3.0",
        "laravel/framework": "~5.1",
        "cviebrock/eloquent-sluggable": "^4.1",
        "venturecraft/revisionable": "1.*"
    },
    "require-dev": {
        "phpunit/phpunit": "~7.0",
        "orchestra/testbench": "^3.6"
    },
    "autoload": {
        "psr-4": {
            "Sirs\\Surveys\\": "src"
